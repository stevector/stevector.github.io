---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>stevector.com</title>
 <link href="http://stevector.com/atom.xml" rel="self"/>
 <link href="http://stevector.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://stevector.com/</id>
 <author>
   <name>Steve Persch</name>
   <email>steve@stevepersch.com</email>
 </author>

 {% for post in site.posts limit:20 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://stevector.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://stevector.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>